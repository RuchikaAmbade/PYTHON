   "source": [
    "def string_length(str1):\n",
    "    count = 0\n",
    "\n",
    "    for char in str1:\n",
    "        count += 1\n",
    "\n",
    "    return count\n",
    "\n",
    "print(string_length('python'))"
   ]
  }
