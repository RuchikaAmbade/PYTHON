   "source": [
    "##### WAP to determine whether a given year is a leap year."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "83b6aa0a-c0cb-4faa-b673-e2208ec4fe47",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "False\n",
      "True\n"
     ]
    }
   ],
   "source": [
    "def leap_year(y):\n",
    "    if y%400 == 0:\n",
    "        return True\n",
    "    if y%100 == 0:\n",
    "        return False\n",
    "    if y%4 == 0:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "print(leap_year(1900))\n",
    "print(leap_year(2004))"
   ]
  },
